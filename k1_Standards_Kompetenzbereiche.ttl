n1:p3b
	 a skos:Concept;
	skos:inScheme n0:;
	skos:notation "L 1.1";
	skos:broader n1:u7p;
	skos:prefLabel "nutzen sinntragende Vorstellungen von rationalen Zahlen, insbesondere von natürlichen, ganzen und gebrochenen Zahlen entsprechend der Verwendungsnotwendigkeit,"@de.
n1:g9e
	 a skos:Concept;
	skos:inScheme n0:;
	skos:notation "L 1.2";
	skos:broader n1:u7p;
	skos:prefLabel "stellen Zahlen der Situation angemessen dar, unter anderem in Zehnerpotenzschreibweise,"@de.
n1:u7v
	 a skos:Concept;
	skos:inScheme n0:;
	skos:notation "L 1.3";
	skos:broader n1:u7p;
	skos:prefLabel "begründen die Notwendigkeit von Zahlbereichserweiterungen an Beispielen,"@de.
n1:q5q
	 a skos:Concept;
	skos:inScheme n0:;
	skos:notation "L 1.4";
	skos:broader n1:u7p;
	skos:prefLabel "nutzen Rechengesetze, auch zum vorteilhaften Rechnen,"@de.
n1:u5c
	 a skos:Concept;
	skos:inScheme n0:;
	skos:notation "L 1.5";
	skos:broader n1:u7p;
	skos:prefLabel "nutzen zur Kontrolle Überschlagsrechnungen und andere Verfahren,"@de.
n1:g7z
	 a skos:Concept;
	skos:inScheme n0:;
	skos:notation "L 1.6";
	skos:broader n1:u7p;
	skos:prefLabel "runden Rechenergebnisse entsprechend dem Sachverhalt sinnvoll,"@de.
n1:c9d
	 a skos:Concept;
	skos:inScheme n0:;
	skos:notation "L 1.7";
	skos:broader n1:u7p;
	skos:prefLabel "verwenden Prozent- und Zinsrechnung sachgerecht,"@de.
n1:g3p
	 a skos:Concept;
	skos:inScheme n0:;
	skos:notation "L 1.8";
	skos:broader n1:u7p;
	skos:prefLabel "erläutern an Beispielen den Zusammenhang zwischen Rechenoperationen und deren Umkehrungen und nutzen diese Zusammenhänge,"@de.
n1:a4e
	 a skos:Concept;
	skos:inScheme n0:;
	skos:notation "L 1.9";
	skos:broader n1:u7p;
	skos:prefLabel "wählen, beschreiben und bewerten Vorgehensweisen und Verfahren, denen Algorithmen bzw. Kalküle zu Grunde liegen,"@de.
n1:r5p
	 a skos:Concept;
	skos:inScheme n0:;
	skos:notation "L 1.10";
	skos:broader n1:u7p;
	skos:prefLabel "führen in konkreten Situationen kombinatorische Überlegungen durch, um die Anzahl der jeweiligen Möglichkeiten zu bestimmen,"@de.
n1:q7s
	 a skos:Concept;
	skos:inScheme n0:;
	skos:notation "L 1.11";
	skos:broader n1:u7p;
	skos:prefLabel "prüfen und interpretieren Ergebnisse in Sachsituationen unter Einbeziehung einer kritischen Einschätzung des gewählten Modells und seiner Bearbeitung."@de.
n1:u7p
	 a skos:Concept;
	skos:inScheme n0:;
	skos:notation "L 1";
	skos:broader n1:e7u;
	skos:prefLabel "Leitidee Zahl"@de; 
	skos:definition "Die Schülerinnen und Schüler ..."@de;
